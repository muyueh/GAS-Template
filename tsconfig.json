{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "ES2021"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "google-apps-script"
    ],
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
